package domain;

import lombok.Data;


@Data
public class domain {
	
		private String food;
		private String rank;
		public domain (String food , String rank) {
			super();
			this.food = food;
			this.rank = rank;
		
}
}


package ExJson;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.Map;

import net.sf.json.JSONObject;

public class cake {

	
public static void main(String[] args) {
	Map<String,Object>food = new HashMap<String,Object>();
	ArrayList<Map<String,String>>rank=new ArrayList<Map<String,String>>();
	Map<String,String>cake;
	cake = new HashMap<String,String>();
	cake.put("paris baguette", "4th");
	cake.put("costco", "2nd");
	cake.put("emart", "3rd");
	cake.put("baskinrobbins", "1st");
	
	rank.add(cake);
	cake = null;
	food.put("brand", rank);
	JSONObject jsonObject = JSONObject.fromObject(food);
	System.out.println(jsonObject);
	
	
}
}
